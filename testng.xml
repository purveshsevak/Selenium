<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">

<listeners>
	<listener class-name="Selenium.Listeners"/>
</listeners>

  <test thread-count="5" name="ValidatingTitle">
    <classes>
      <class name="Selenium.ValidateTitle"/>
    </classes>
  </test>
  <test thread-count="5" name="NavBarValidation">
    <classes>
      <class name="Selenium.ValidateNavigationBar"/>
    </classes>
  </test>
  <test thread-count="5" name="VerifyHomePage">
    <classes>
      <class name="Selenium.HomePage"/>
    </classes>
  </test>
  
</suite>
